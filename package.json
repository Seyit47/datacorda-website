{
  "name": "nuxt3-starter",
  "version": "0.1.0",
  "description": "a Nuxt 3 starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "prepare": "husky install",
    "clean": "rm -rf .nuxt dist .output",
    "generate:locales": "node tools/translator.js ./locales en.yml"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxtjs/device": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^6.6.7",
    "@pinia/nuxt": "^0.4.11",
    "@types/aos": "^3.0.4",
    "@vueuse/core": "^9.11.1",
    "@vueuse/nuxt": "^9.11.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.5.3",
    "nuxt-icon": "^0.4.2",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "postcss-custom-properties": "^13.2.0",
    "prettier": "^2.7.1",
    "sass": "^1.63.6",
    "serve": "^14.2.0",
    "typescript": "^5.1.3",
    "vue-tsc": "^1.8.1"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "yarn lintfix"
    ]
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "aos": "^2.3.4",
    "connect-redis": "^7.1.0",
    "gsap": "^3.12.2",
    "vee-validate": "^4.10.2",
    "vue-toastification": "^2.0.0-rc.5",
    "yup": "^1.2.0"
  }
}
